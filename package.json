{
  "name": "app-alojamiento-tmp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:ssr:AppAlojamientoTmp",
    "start:dev": "ng serve --proxy-config proxy.conf.json",
    "start:no-proxy": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "start:prod": "npm run build && npm run serve:ssr:AppAlojamientoTmp",
    "serve:ssr:AppAlojamientoTmp": "node dist/AppAlojamientoTmp/server/server.mjs",
    "backend:proxy": "node scripts/simple-proxy.mjs",
    "start:all": "concurrently -n proxy,ng \"npm run backend:proxy\" \"npm run start:dev\""
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@angular/ssr": "^20.3.7",
    "express": "^5.1.0",
    "mapbox-gl": "^3.16.0",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.26.3",
    "tslib": "^2.3.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.7",
    "@angular/cli": "^20.3.7",
    "@angular/compiler-cli": "^20.3.0",
    "@types/express": "^5.0.1",
    "@types/mapbox-gl": "^3.4.1",
    "@types/node": "^20.17.19",
    "concurrently": "^9.2.1",
    "typescript": "~5.9.2"
  }
}
